{
  "printWidth": 120,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "trailingComma": "none",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)|^(.*react-*(.*)$)|^(.*redux-*(.*)$)",
    "^(aos$)|^(framer-motion$)|^(lodash$)",
    "^(markdown*$)|^(moment$)",
    "^(tailwind*)",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}